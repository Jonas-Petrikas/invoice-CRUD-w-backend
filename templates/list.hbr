<main>

    <section class="content-section">
        <div class="list-container">
            <h1>Sąskaitų sąrašas</h1>
            {{#each invoices.items}}
            <div class="list-item">
                <div>{{this.number}}</div>
                <div>{{this.date}}</div>
                <div>{{this.grandTotal}} Eur</div>
                <a href="{{URL}}read/{{this.number}}"><button>peržiūrėti</button></a>
            </div>

            {{/each}}
        </div>
    </section>
</main>